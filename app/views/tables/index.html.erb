<p id="notice"><%= notice %></p>

<h1><%= t('.title') %></h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th><%= t('.name') %></th>
      <th><%= t('.discount') %></th>
      <th><%= t('.expiration_date') %></th>
      <th><%= t('.status') %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tables.each do |table| %>
      <tr>
        <td><%= table.name %></td>
        <td><%= table.discount %></td>
        <td><%= table.expiration_date %></td>
        <td><%= table.status %></td>
        <td><%= link_to t('.show'), table %></td>
        <td><%= link_to t('.edit'), edit_table_path(table) %></td>
        <td><%= link_to t('.destroy'), table, method: :delete, data: { confirm: t('.confirm')} %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('.new'), new_table_path %>
